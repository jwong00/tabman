{
  "browser_action": {
    "browser_style": true,
    "default_title": "tab manager",
    "default_popup": "popup.html"
  },

  "manifest_version": 2,
  "name": "tabMan",
  "version": "0.1",

  "description": "Tab-management extension.",

  "icons": {
    "48": "dynamic_feed_FILL0_wght400_GRAD0_opsz48.png"
  },

  "permissions": [
      "<all_urls>",
      "tabs"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["tabman.js"]
    }
  ]

}
